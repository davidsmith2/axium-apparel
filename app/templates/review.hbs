<div class="col-sm-2">
    <figure class="pull-right">
        <img src="images/{{user.avatar}}">
        <figcaption>{{user.name}}</figcaption>
    </figure>
</div>
<div class="col-sm-8">
    <div class="clearfix">
        <div class="pull-left margin-right-1">
            <h3 class="margin-top-0">{{title}}</h3>
        </div>
        <div class="pull-left margin-right-1">
            {{date}}
        </div>
        <div class="pull-left">
            {{#renderStars star_rating}}{{/renderStars}}
        </div>
    </div>
    <div>
        <p>{{description}}</p>
    </div>
</div>
